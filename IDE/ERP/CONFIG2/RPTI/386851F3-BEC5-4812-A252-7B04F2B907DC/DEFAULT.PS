  procedure TERPFormRpti_OnChart(aOwner:TComponent; aParent:TWinControl);
  var
    lkPC :TxcPageControlEx;
    lkTS :TxcTabSheet;
  begin
    lkPC := CreateTxcPageControlEx(aOwner, aParent, 'PCCharts', 0,0,600,600, alClient);
    lkPC.MultiLine := false;
    lkTS := TERPFormRpti_CreateTabSheetChartBar(aOwner,lkPC,'CS_COUNT','По кол-ву контрагентов','Сравнительный график количества контрагентов от года рождения'   ,TERPForm_DBGridMain(aOwner).Columns[0].FieldName);
    with TBarSeries(aOwner.FindComponent('TBarSeries_CS_COUNT'))do
      begin
      Color            := Amunhotep.MainForm.StyleManager.Colors.HotTrackBorder;
      ColorEachPoint   := false;
      YValues.Order    := loNone;
      YValues.DateTime := false;
      end;
    lkTS := TERPFormRpti_CreateTabSheetChartBar(aOwner,lkPC,'SUMPC'   ,'По сумме процентов'    ,'Сравнительный график суммы погашенных процентов от года рождения',TERPForm_DBGridMain(aOwner).Columns[0].FieldName);
    with TBarSeries(aOwner.FindComponent('TBarSeries_SUMPC'))do
      begin
      Color            := Amunhotep.MainForm.StyleManager.Colors.HotTrackBorder;
      ColorEachPoint   := false;
      YValues.Order    := loNone;
      YValues.DateTime := false;
      end;
    TxcPageControlEx_ActivateDefaultPage(lkPC);
  end;
