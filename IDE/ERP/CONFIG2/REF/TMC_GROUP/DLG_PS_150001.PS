{$IFNDEF DB:CONFIG/REF/TMC_GROUP/DLG_PS.INC} {$I ./DLG_PS.INC} {$ENDIF}
var
  lkTmcGroupId :string;
begin
  if(DialogTMCGroup_Execute('Выберите группу ТМЦ', '150001', lkTMCGroupId))then
    SetGlobalVariable('TMC_GROUP_ID', lkTmcGroupId)
   else
    SetGlobalVariable('TMC_GROUP_ID', '0');
end.  
