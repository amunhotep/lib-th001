  //============================================================================
  procedure dtsJournal_AfterScroll(DataSet :TDataSet);
  var
    lkTypeId, i :Integer;
  begin
    lkTypeId := DataSet.FieldByName('TYPE_ID').AsInteger;
    DocForm.Caption := DataSet.FieldByName('TYPE_ID_').AsString+' '+DataSet.FieldByName('DOCNUMBERSTR').AsString;
    DocForm.SetCaption(DocForm.Caption);
    TopBar.Caption := #$20#$20#$20#$20#$20#$20+DocForm.Caption;
    PanelClient.Colors.Window := DataSet.FieldByName('COLOR_BGR').AsInteger;
    PanelClient.Font.Color    := DataSet.FieldByName('COLOR_FRG').AsInteger;
    dbgDoc.ReadOnly := (DataSet.FieldByName('FLAG_COMMIT').AsInteger = 1);
    PanelClient.Enabled := not dbgDoc.ReadOnly;
    if dbgDoc.ReadOnly then
      dbgDoc.Options := [dgRowSelect, dgAlwaysShowSelection,dgTitles,dgIndicator,dgColumnResize,dgColLines,dgRowLines,dgTabs] 
     else
      begin
      dbgDoc.Options := [dgEditing,dgTitles,dgIndicator,dgColumnResize,dgColLines,dgRowLines,dgTabs,dgConfirmDelete,dgCancelOnExit,dgAlwaysShowSelection,dgMultiSelect];
      if( (lkTypeId = 1000027) or (lkTypeId = 1000029) )then
        for i:=0 to dbgDoc.Columns.Count-1 do
          if(dbgDoc.Columns[i].FieldName = 'PRICE_TMC')then
            begin
            dbgDoc.Columns[i].ReadOnly := false;
            dbgDoc.Columns[i].Color    := dbgDoc.Color;
            break;
            end;
      end; 
    if(   (lkTypeId = 1000026) or (lkTypeId = 1000028) or (lkTypeId = 1000071) or (lkTypeId = 1000075) 
       or (lkTypeId = 1000078) or (lkTypeId = 1000079) or (lkTypeId = 1000135) or (lkTypeId = 1000136) 
       or (lkTypeId = 1000129) or (lkTypeId = 1000027)
       )then
      TCustomAction(DataSet.Owner.FindComponent('actDocNew')).Visible := true;
    if( (lkTypeId = 1000026) or (lkTypeId = 1000128) or (lkTypeId = 1000071) or (lkTypeId = 1000135))then
      begin
      TCustomAction(DataSet.Owner.FindComponent('actTMCAddSum')).Visible := true;
      TCustomAction(DataSet.Owner.FindComponent('actDocReCalcMove')).Visible := true;
      end;
    if( (lkTypeId = 1000024) or (lkTypeId = 1000027) or (lkTypeId = 1000029) or (lkTypeId = 1000142) )then
      begin
      dbeCS_ID_.Visible       := false; 
      dbeCS_ADDR_ID_.Visible  := false; 
      dbeCS_DOC_ID_.Visible   := false; 
      dbeWRK_ID_.Visible      := false; 
      dbcbxTAX_ID.Visible     := (lkTypeId = 1000142); 
      dbeTMC_GRID_ID_.Visible := false; 
      dbeJ_PAY_ID_.Visible    := false;
      dbeTO_PLACE_ID_.Visible := (lkTypeId = 1000024); 
      PanelClient.Height := dbePLACE_ID_.Top + dbePLACE_ID_.Height + 4;
      end
     else
      begin
      dbeTO_PLACE_ID_.Visible := false;      
      end;   
    TLabel(DataSet.Owner.FindComponent('lblPlaceTo')).Visible := dbeTO_PLACE_ID_.Visible;
    TWinControl(dbcbxPayment).Visible := ( (lkTypeId = 1000026) or (lkTypeId = 1000028) );   
  end;
