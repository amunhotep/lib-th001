  //============================================================================
  procedure actTMCSetPriceIn_OnExecute(Sender :TObject);
  var
    lkSQL :string;
  begin
    if not dtsJournal.Active then exit;
    lkSQL := 
      'EXECUTE BLOCK AS '+SQLCRLF+
      '  DECLARE VARIABLE P_J_ID   TYPE OF COLUMN TABL$D_1000014.J_ID; '+SQLCRLF+
      '  DECLARE VARIABLE P_D_ID   TYPE OF COLUMN TABL$D_1000014.ID; '+SQLCRLF+
      '  DECLARE VARIABLE P_TMC_ID TYPE OF COLUMN TABL$D_1000014.TMC_ID; '+SQLCRLF+
      '  DECLARE VARIABLE P_PRICE  TYPE OF COLUMN TABL$D_1000014.PRICE; '+SQLCRLF+
      'BEGIN '+SQLCRLF+
      '  P_J_ID = '+dtsJournal.FieldByName('ID').AsString+'; '+SQLCRLF+
      '  FOR '+SQLCRLF+
      '    SELECT D.ID, D.TMC_ID '+SQLCRLF+
      '    FROM   TABL$D_1000014 D '+SQLCRLF+
      '    WHERE  (D.J_ID = :P_J_ID) '+SQLCRLF+
      '    INTO   :P_D_ID, :P_TMC_ID '+SQLCRLF+
      '  DO '+SQLCRLF+
      '    BEGIN '+SQLCRLF+
      '    P_PRICE = 0; '+SQLCRLF+
      '    SELECT FIRST 1 COALESCE(P.PRICE_IN, 0) '+SQLCRLF+
      '    FROM   TABL$R_TMC_P P '+SQLCRLF+
      '    WHERE  (P.TMC_ID = :P_TMC_ID) '+SQLCRLF+
      '      AND  (P.VALUE_DATE = (SELECT MAX(P1.VALUE_DATE) FROM TABL$R_TMC_P P1 WHERE (P1.TMC_ID = P.TMC_ID)AND(P1.PRICE_IN <> 0)) )'+SQLCRLF+
      '    INTO   :P_PRICE; IF(:P_PRICE IS NULL)THEN P_PRICE = 0; '+SQLCRLF+
      '    UPDATE TABL$D_1000014 D1 SET D1.PRICE = :P_PRICE WHERE (D1.ID = :P_D_ID); '+SQLCRLF+
      '    END '+SQLCRLF+
      'END';
    CloseDataSets;
    DocForm_ExecSQLParams([], lkSQL, [],[]);
    RefreshView;
  end;
