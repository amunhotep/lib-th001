{$DEFINE PEAKTOP:IDE/ERP/DBO/REF/DIALOGCASEFOLDER.PS}
{$IFNDEF PEAKTOP:IDE/ERP/DBO/$COMMON/DIALOG_CASEOBJECTFOLDER.INC}
  {$I ../$COMMON/DIALOG_CASEOBJECTFOLDER.INC}
{$ENDIF}
var
  lkTypeId :string;
begin
  lkTypeId := IntToStr(ERP_OBJECTTYPE_OPER);
  if ERPDialogCaseObjFolder_Execute(ERP_OBJECTTYPE_OPER, lkTypeId) then
    SetGlobalVariable('TYPE_ID', lkTypeId)
   else
    SetGlobalVariable('TYPE_ID', 'NULL');
end.  
