{$DEFINE PEAKTOP:IDE/ERP/DBO/OPER/IMG/DEFAULT.PS}
{$IFNDEF PEAKTOP:IDE/ERP/DBO/OPER/IMG/DEFAULT.INC} {$I PEAKTOP:IDE/ERP/DBO/OPER/IMG/DEFAULT.INC} {$ENDIF}
var
  OBJECT_ID :string;
  RECORD_ID :string;
begin
  OBJECT_ID := GetGlobalVariable(ERP_GLOBALVARIABLE_OBJ_ID);
  RECORD_ID := GetGlobalVariable(ERP_GLOBALVARIABLE_REC_ID);
  if( (StrTrimAll(OBJECT_ID) <> '') and (StrTrimAll(RECORD_ID) <> '') )then
    TERPFormOperIMG_Create(OBJECT_ID, RECORD_ID);
  SetGlobalVariable(ERP_GLOBALVARIABLE_OBJ_ID, '');  
  SetGlobalVariable(ERP_GLOBALVARIABLE_REC_ID, '');  
end.
