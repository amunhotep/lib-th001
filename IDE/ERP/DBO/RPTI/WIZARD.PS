{$DEFINE PEAKTOP:IDE/ERP/DBO/RPTI/WIZARD.PS}
{$IFNDEF PEAKTOP:IDE/ERP/DBO/RPTI/WIZARD.INC}  {$I ./WIZARD.INC} {$ENDIF}
var 
  lkGUID_       :TGUID;
  lkGUID        :string; 
  lkCaption     :string;
  lkScriptName  :string;
  lkParentTypeId:string;
  lkInDataBase  :Boolean;
begin
  lkParentTypeId := '5';
  CoCreateGUID(lkGUID_);
  lkGUID := GUIDToStr(lkGUID_);
  if ERP_WizardRPTI_Execute(lkGUID, lkCaption, lkParentTypeId, lkInDataBase)then
    if ERP_ObjCreate(lkGUID, lkCaption, lkParentTypeId, lkInDataBase, lkScriptName) then
      Amunhotep.MainForm.ProcessFile(lkScriptName);  
end.
