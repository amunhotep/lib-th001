      1 :Result := ERP_FILENAME_BMP_PREVIEW;
      2 :Result := ERP_FILENAME_BMP_PREVIEW_REF;
      3 :Result := ERP_FILENAME_BMP_PREVIEW_OPER;
      4 :Result := ERP_FILENAME_BMP_PREVIEW_JRNL;
      5 :Result := ERP_FILENAME_BMP_PREVIEW_RPT;
