  //============================================================================
  function ERP_ObjectPreviewIcon(const aID :Integer):string;
  begin
    Result := ERP_FILENAME_ICO_PREVIEW;
    case aID of
      1 :Result := ERP_FILENAME_ICO_PREVIEW;
      2 :Result := ERP_FILENAME_ICO_PREVIEW_REF;
      3 :Result := ERP_FILENAME_ICO_PREVIEW_OPER;
      4 :Result := ERP_FILENAME_ICO_PREVIEW_DOC;
      5 :Result := ERP_FILENAME_ICO_PREVIEW_RPT;
      6 :Result := ERP_FILENAME_ICO_PREVIEW_REG;
    end;
  end;
  //============================================================================
  function ERP_ObjectPreviewBitmap(const aID :Integer):string;
  begin
    Result := ERP_FILENAME_BMP_PREVIEW;
