{$DEFINE PEAKTOP:IDE/ERP/DBO/REF/OBJECTEDITOR.PS}
{$IFNDEF PEAKTOP:IDE/ERP/DBO/$COMMON/FORMERPEDTR.INC} {$I ../$COMMON/FORMERPEDTR.INC} {$ENDIF}
var
  OBJECT_ID :string;
begin
  OBJECT_ID := GetGlobalVariable(ERP_GLOBALVARIABLE_OBJ_ID);
  if(StrTrimAll(OBJECT_ID) <> '')then
    TERPFormEdtr_Create(OBJECT_ID, ERP_OBJECTTYPE_REF);  
  SetGlobalVariable(ERP_GLOBALVARIABLE_OBJ_ID, '');  
end.
